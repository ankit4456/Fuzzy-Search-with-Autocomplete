{
  "version": 2,
  "builds": [
    {
      "src": "Fe/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Be/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Fe/$1"
    }
  ]
}

